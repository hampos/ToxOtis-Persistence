<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
        dynamic-insert="true"
        dynamic-update="true"
        mutable="true"
        name="org.opentox.toxotis.ontology.ResourceValue"
        optimistic-lock="version"
        polymorphism="implicit"
        select-before-update="false"
        table="Resource">
        <id name="hash" column="hash"/>
        <property name="uri" column="uri" type="org.opentox.toxotis.persistence.usertypes.VRIUserType" unique="true"/>
        <many-to-one name="ontologicalClass" class="org.opentox.toxotis.ontology.OntologicalClass" cascade="all">
            <column name="resourceType_NS"/>
            <column name="resourceType_Name" length="50"/>
        </many-to-one>
    </class>
</hibernate-mapping>
