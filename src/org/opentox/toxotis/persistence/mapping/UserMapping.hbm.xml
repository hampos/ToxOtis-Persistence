<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="true" dynamic-update="true" mutable="true" name="org.opentox.toxotis.core.component.User" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id name="uid" type="string"/>
    <property name="name" type="string"/>
    <property name="mail" type="string"/>
    <property column="password" name="hashedPass" type="string"/>
  </class>
</hibernate-mapping>
