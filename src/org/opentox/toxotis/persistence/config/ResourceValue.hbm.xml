<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
        name="org.opentox.toxotis.ontology.ResourceValue"
        table="Resource"
        dynamic-insert="false"
        dynamic-update="false"
        mutable="true"
        optimistic-lock="version"
        polymorphism="implicit"
        select-before-update="false">
        <id column="rsId" type="int">
            <generator class="native"/>
        </id>
        <property name="uri" column="URI" type="org.opentox.toxotis.persistence.usertypes.VRIUserType"/>
        <property name="ontologicalClass" column="CLASS" type="org.opentox.toxotis.persistence.usertypes.OntologicalClassUserType"/>
    </class>

</hibernate-mapping>
